}

.moveLeft1 {
  animation: mLeft1 1s forwards;
}
@keyframes mLeft1 {
  from {
    left: 0%;
  }
  to {
    left: -100%;
  }
}
.moveLeft2 {
  animation: mLeft2 1s forwards;
}
@keyframes mLeft2 {
  from {
    left: -100%;
  }
  to {
    left: 0%;
  }
}


.moveRight1 {
  animation: mRight1 1s forwards;
}
@keyframes mRight1 {
  from {
    right: 0%;
  }
  to {
    right: -100%;
  }
}
.moveRight2 {
  animation: mRight2 1s forwards;
}
@keyframes mRight2 {
  from {
    right: -100%;
  }
  to {
    right: 0%;
  }
}

.moveCenter1 {
  animation: mCenter1 1s forwards;
}
@keyframes mCenter1 {
  from {
    bottom: 0%;
  }
  to {
    bottom: -100%;
  }
}
.moveCenter2 {
  animation: mCenter2 1s forwards;
}
@keyframes mCenter2 {
  from {
    bottom: -100%;
  }
  to {
    bottom: 0%;
  }
}

