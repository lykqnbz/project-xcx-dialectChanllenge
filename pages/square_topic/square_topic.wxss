.squareTopic{
  width: 100%;
  background: #27262C;
  box-shadow: 0 4rpx 8rpx 0 rgba(0,0,0,0.25);
  min-height:148rpx;
  border-radius: 0px 0px 20rpx 20rpx;
  box-sizing: border-box;
  padding: 34rpx 30rpx;
  font-size: 34rpx;
  color: #fff;
  font-weight: 600;
  line-height: 50rpx;
  position: relative;
  z-index: 2;
  display: flex;
  justify-content: flex-start;
  text-align: justify;
}
.squareList{
  width: 92%;
  margin-left: 4%;
}
.sqListItem{
  width: 690rpx;
  height: 280rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #fff;
  overflow: hidden;
  border-radius: 20rpx;
  margin-top: 24rpx;
}
.sqlItem_top{
  background: #FFFAE5;
  width: 100%;
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0rpx 28rpx;
  font-size: 28rpx;
  color: #1A1E3B;
  align-items: center;
  position: relative;
}
.sqlItem_top>label,.sqlItem_top>navigator{
  position: relative;
  z-index: 1;
}
.stN{
  width: 100%;
  position: absolute;
  height: 192rpx;
  z-index: 0;
  display: none;
  top: 0px;
  left: 0px;
}
.squareList .sqListItem:nth-child(1) .stN1,.squareList .sqListItem:nth-child(2) .stN2,.squareList .sqListItem:nth-child(3) .stN3{
    display: block;
}
.sqlItem_center{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.sqlItemCenRight {
  background: #ffe26a;
  border-radius: 100rpx;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 68rpx;
  box-sizing: border-box;
  min-width: 200rpx;
  margin-right: 28rpx;
  position: relative;
  z-index: 20;
}
.sqlItemCenLeft{
  margin-left: 28rpx;
  font-size: 42rpx;
  font-weight: 600;
  color: #1A1E3B;
  display: flex;
  align-items: center;
  position: relative;
  z-index: 20;
}
.sqlItemCenLeft>image{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
  margin-right: 16rpx;
}
.sqlItemCenLeft>view{
  position: absolute;
  left: 64rpx;
  top: 54rpx;
}
.sqlItemCenLeft>view>image{
  width: 38rpx;
  height: 38rpx;
}

.sqlItemCenRight>label {
  color: #1a1e3b;
  font-size: 28rpx;
  margin-right: 10rpx;
}

.sqlItemCenRight>image {
  width: 28rpx;
  height: 32rpx;
  margin-left: 20rpx;
}
.sqlItem_bot{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 24rpx;
}
.sqlItem_bot>label{
  font-size: 30rpx;
  color: #1a1e3b;
  margin-left: 28rpx;
}
.sqlItem_bot>view{
  display: flex;
  min-width: 200rpx;
  justify-content: space-between;
   margin-right: 28rpx; 
}
.sqlItem_bot>view>view{
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #888;
}
.sqlItem_bot>view>view>image{
  width: 33rpx;
  height: 34rpx;
  margin-right: 10rpx;
}
.sqlItem_bot>view>label{
  font-size: 30rpx;
  color: #888;
  margin-left: 30rpx;
}

