page {
  height: 100%;
  background: #f5f5f9;
}

.psnPhoto_container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.psnP_btn {
  background: #6a98ff;
  width: 100%;
  height: 98rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.psnP_btn>label {
  height: 68rpx;
  width: 2rpx;
  background: #fff;
}

.psnP_btn>view {
  width: 49%;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-size: 34rpx;
  color: #fff;
  height: 98rpx;
  line-height: 98rpx;
}

.psnP_list {
  width: 694rpx;
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}

.psnP_item {
  width: 218rpx;
  height: 218rpx;
  position: relative;
  margin-bottom: 20rpx;
}

.psnP_list>.psnP_item:nth-child(3n-1) {
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.psnP_list>.psnP_item:nth-child(n+9) {
  display: none;
}
.psnP_item>image {
  width: 218rpx;
  height: 218rpx;
}

.item_rigt {
  width: 38rpx;
  height: 38rpx;
  background: #fff;
  border: 1rpx solid #a4a4a4;
  border-radius: 50%;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.item_rigt>label{
  width: 20rpx;
  height: 20rpx;
  background: #6A98FF;
  border-radius: 50%;
}
