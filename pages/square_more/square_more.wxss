  box-shadow: 0 4rpx 8rpx 0 rgba(0,0,0,0.25);
  min-height:148rpx;
  border-radius: 0px 0px 20rpx 20rpx;
  box-sizing: border-box;
  padding: 34rpx 30rpx;
  font-size: 34rpx;
  color: #fff;
  font-weight: 600;
  line-height: 50rpx;
  position: relative;
  z-index: 2;
  text-align: justify;
}
.squareList{
  width: 92%;
  margin-left: 4%;
}

.sqListItem{
  width: 690rpx;
  height: 208rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #fff;
  overflow: hidden;
  border-radius: 20rpx;
  margin-top: 24rpx;
}

.sqlItem_center{
  margin-top: 10rpx;
