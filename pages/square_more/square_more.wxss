  min-width: 200rpx;
  margin-right: 28rpx;
}
.sqlItemCenLeft{
  margin-left: 28rpx;
  font-size: 34rpx;
  color: #000;
  display: flex;
  align-items: center;
}
.sqlItemCenLeft>view{
  position: relative;
  width: 97rpx;
  height: 110rpx;
  margin-right: 16rpx;
}
.sqlItemCenLeft>label{
  max-width: 8em;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.sqlItemCenLeft>view>view{
    position: absolute;
    top: 62rpx;
    left: 63rpx;
    z-index: 20;
}
.sqlItemCenLeft>view>view>image{
    width: 38rpx;
    height: 38rpx;
}

